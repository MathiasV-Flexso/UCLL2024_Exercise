        <App id="app" autoFocus="false">
            <f:FlexibleColumnLayout id="fcl" layout="{layout>/layout}" backgroundDesign="Translucent">
            </f:FlexibleColumnLayout>
        </App>
    </Shell>
</mvc:View>
